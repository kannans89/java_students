<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	   					   http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="triangle" class="com.techlabs.autowiring.Triangle"
		autowire="byName">
	</bean>

	<bean name="pointA" class="com.techlabs.autowiring.Point">
		<property name="x" value="5"></property>
		<property name="y" value="5"></property>
	</bean>
	<bean name="pointB" class="com.techlabs.autowiring.Point">
		<property name="x" value="0"></property>
		<property name="y" value="0"></property>
	</bean>
	<bean name="pointC" class="com.techlabs.autowiring.Point">
		<property name="x" value="10"></property>
		<property name="y" value="0"></property>
	</bean>

</beans>