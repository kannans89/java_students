<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="true" />
	<package name="dhdh" extends="struts-default">

		<action name="Add" class="com.techlabs.action.AddAction"
			method="execute">
			<result name="input">/index.jsp</result>
			<result name="success" type="redirect">/Display</result>
		</action>

		<action name="Display" class="com.techlabs.action.DisplayAction"
			method="execute">
			<result name="success">/WEB-INF/view/display.jsp</result>
		</action>



		<action name="">
			<result> /index.jsp</result>
		</action>

	</package>
</struts>