<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    
    <bean name="triangle" class="com.techlabs.autowiring.Triangle" autowire="byName"/>
    
    <bean name="pointA" class="com.techlabs.autowiring.Point">
    <property name="x">
    <value>300</value>
    </property>
     <property name="y">
    <value>200</value>
    </property>
    </bean>
    
    <bean name="pointB" class="com.techlabs.autowiring.Point">
    <property name="x">
    <value>600</value>
    </property>
     <property name="y">
    <value>400</value>
    </property>
    </bean>
    
    <bean name="pointC" class="com.techlabs.autowiring.Point">
    <property name="x">
    <value>1024</value>
    </property>
     <property name="y">
    <value>708</value>
    </property>
    </bean>
    </beans>